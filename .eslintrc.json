{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "no-unused-vars": 1,
    "no-plusplus": 1,
    "no-return-assign": 1,
    "no-useless-escape": 1,
    "no-unused-expressions": 1,
    "guard-for-in": 1,
    "no-continue": 1,
    "no-mixed-operators": 1,
    "import/no-unresolved": 1,
    "no-console": 1,
    "arrow-body-style": 1,
    "default-case": 1,
    "require-yield": 1,
    "no-undef": 1,
    "prefer-rest-params": 1,
    "import/no-extraneous-dependencies": 1,
    "prefer-spread": 1,
    "no-cond-assign": 1,
    "import/no-dynamic-require": 1,
    "no-underscore-dangle": 1,
    "no-multi-assign": 1,
    "class-methods-use-this": 1,
    "camelcase": 1,
    
    "prefer-destructuring": 0,

    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "global-require": 0,
    
    "max-len": 0,
    "no-shadow": 0,
    "func-names": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0
  }
}
